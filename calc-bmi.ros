(defpackage :ros.script.calc-bm.3845550042
  (:use :cl :calc-bmi))
(in-package :ros.script.calc-bm.3845550042)

(defun main (height weight &rest argv)
  (declare (ignorable argv))
  (format t "BMI: ~A~%"
          (calc-bmi:calc-bmi (parse-integer height)
                             (parse-integer weight))))
